}

/* Responsive Menu
--------------------------------------------- */

#responsive-menu-icon {
	cursor: pointer;
	display: none;
	padding: 17px 0 18px;
	padding: 1.7rem 0 1.8rem;
	text-align: center;
}

#responsive-menu-icon::before {
	-webkit-font-smoothing: antialiased;
